<h3>Hello <%= @game_master.name %> This is your party today</h3>
<div class="container">
  <div class="row">
    <% @players.each do |player| %>
      <%= render player %>
    <% end %>
  </div>

  <div class="row">
    <%= button_to "End Session", @game_master, method: :delete, class: "btn"  %>
  </div>
</div>
